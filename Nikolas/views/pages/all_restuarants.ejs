<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>

<body class="container"></body>

<header>
    <%- include('../partials/header'); %>
</header>

<main>

<div class="row">
    <div class="col-sm-8">
        <div class="jumbotron">

            <h1><b><%= locals.tagline %></b></h1>
            <ol>
                
                <% if (id) { %>

                    <% id.forEach(function(user) { %>

                        <li style="font-family:Garamond;">
                            <%= user["firstname"] %>
                            <%= user["lastname"] %>
                        </li>

                    <% }); %>
                   
                <% }; %>
                
            </ol>
        </div>
    </div>
</div>

</main>

<footer>
    <%- include('../partials/footer'); %>
</footer>

</body>
</html>